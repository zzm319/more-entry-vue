{
    "printWidth": 80, 
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "disableLanguages": ["vue"],
    "endOfLine": "auto",
    "ignorePath": ".prettierignore",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "requireConfig": false,
    "trailingComma": "none"
  }
   